<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.notework.dao.NoteDao">
		
	<select id="getList" resultType="com.notework.entity.Note">
		select * from Note
		limit ${(_parameter-1)*15}, 15
	</select>
		
	<select id="get" resultType="com.notework.entity.Note">
		select * from Note where id=#{id}
	</select>
	
</mapper>